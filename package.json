    "url": "git+https://github.com/aaronlopz/jquery-bg-pexels.git"
  },
  "keywords": [
    "jquery",
    "plugin",
    "cover",
    "fullpage",
    "pexels"
  ],
  "author": "Aaron Lopez <aaronlo2012@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aaronlopz/jquery-bg-pexels/issues"
  },
  "homepage": "https://github.com/aaronlopz/jquery-bg-pexels#readme",
  "devDependencies": {
    "eslint": "4.16.0",
    "expect.js": "0.3.1",
    "jquery": "3.3.1",
    "jsdom": "11.5.1",
    "mocha": "5.0.0",
    "pre-commit": "1.2.2"
  },
  "pre-commit": [
    "test"
  ],

  "nyc":{
    "check-coverage": true,
